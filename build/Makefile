.NOTPARALLEL:

ifndef config
  config=debug
endif

ifndef verbose
  SILENT = @
endif

ifeq ($(config),debug)
  LuaPbIntf_config = debug
endif
ifeq ($(config),release)
  LuaPbIntf_config = release
endif

PROJECTS := LuaPbIntf

.PHONY: all clean help $(PROJECTS) 

all: $(PROJECTS)

LuaPbIntf:
ifneq (,$(LuaPbIntf_config))
	@echo "==== Building LuaPbIntf ($(LuaPbIntf_config)) ===="
	@${MAKE} --no-print-directory -C . -f LuaPbIntf.make config=$(LuaPbIntf_config)
endif

clean:
	@${MAKE} --no-print-directory -C . -f LuaPbIntf.make clean

help:
	@echo "Usage: make [config=name] [target]"
	@echo ""
	@echo "CONFIGURATIONS:"
	@echo "  debug"
	@echo "  release"
	@echo ""
	@echo "TARGETS:"
	@echo "   all (default)"
	@echo "   clean"
	@echo "   LuaPbIntf"
	@echo ""
